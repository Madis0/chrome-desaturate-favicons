{
    "manifest_version": 2,
    "name": "Desaturate Favicons",
    "description": "Desaturate the favicons of all inactive tabs.",
    "version": "0.0.3",
    "permissions": [
        "<all_urls>",
        "tabs"
    ],
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "icons": {
        "48": "icons/icon48.png",
        "72": "icons/icon72.png",
        "96": "icons/icon96.png"
    }
}
